{
	"$schema": "https://millennium.pyke.io/v1",
	"build": {
		"distDir": ["./index.html"],
		"devPath": ["./index.html"]
	},
	"millennium": {
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "io.pyke.hello-world",
			"icon": [
				"../../.icons/32x32.png",
				"../../.icons/128x128.png",
				"../../.icons/256x256.png",
				"../../.icons/icon.ico"
			]
		},
		"allowlist": {
			"all": false
		},
		"windows": [
			{
				"title": "Millennium Example",
				"width": 600,
				"height": 600,
				"resizable": true,
				"fullscreen": false,
				"center": true
			}
		],
		"security": {
			"csp": {
				"default-src": "millennium: millennium.localhost 'unsafe-inline' 'self'"
			}
		}
	}
}
