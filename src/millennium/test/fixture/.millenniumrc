{
	"build": {
		"distDir": "./src",
		"devPath": "http://localhost:4000"
	},
	"millennium": {
		"bundle": {
			"identifier": "com.millennium.example",
			"active": true
		},
		"allowlist": {
			"all": true
		},
		"windows": [
			{
				"title": "Millennium App"
			}
		],
		"security": {
			"csp": "default-src blob: data: filesystem: ws: wss: http: https: millennium: 'unsafe-eval' 'unsafe-inline' 'self'"
		},
		"updater": {
			"active": false
		}
	}
}
