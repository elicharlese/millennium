{
	"[rust]": {
		"editor.formatOnSave": true,
		"editor.semanticHighlighting.enabled": true
	},
	"[mdx]": {
		"editor.wordWrap": "on",
		"editor.insertSpaces": true
	},

	"rust.checkOnSave.command": "clippy",
	"rust.rustfmt.nightly": true,
	"rust.cargo.allFeatures": true,
	"rust.cachePriming.enable": true
}
