{
	"$schema": "https://millennium.pyke.io/v1",
	"package": {
		"productName": "{{ app_name }}",
		"version": "0.1.0"
	},
	"build": {
		"distDir": "{{ dist_dir }}",
		"devPath": "{{ dev_path }}"
	},
	"millennium": {
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.millennium.dev",
			"icon": [
				"./icons/32x32.png",
				"./icons/128x128.png",
				"./icons/256x256.png",
				"./icons/icon.ico"
			]
		},
		"allowlist": {
			"all": false
		},
		"windows": [
			{
				"title": "{{ window_title }}",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false,
				"center": true
			}
		],
		"security": {
			"csp": {
				"default-src": "blob: data: filesystem: ws: wss: http: https: millennium: 'unsafe-eval' 'unsafe-inline' 'self'",
				"style-src-elem": "https: 'unsafe-inline'"
			}
		}
	}
}
